{
  "name": "@my-touring-links/app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@andybeersdev/element-resize-detector": "^1.0.1",
    "@googlemaps/extended-component-library": "^0.4.3",
    "@material/typography": "^14.0.0",
    "@playwright/test": "^1.28.1",
    "@smui/button": "^7.0.0-beta.15",
    "@smui/checkbox": "^7.0.0-beta.15",
    "@smui/chips": "^7.0.0-beta.15",
    "@smui/dialog": "^7.0.0-beta.15",
    "@smui/fab": "^7.0.0-beta.15",
    "@smui/form-field": "^7.0.0-beta.15",
    "@smui/icon-button": "^7.0.0-beta.15",
    "@smui/list": "^7.0.0-beta.15",
    "@smui/menu": "^7.0.0-beta.15",
    "@smui/slider": "^7.0.0-beta.15",
    "@smui/tab": "^7.0.0-beta.15",
    "@smui/tab-bar": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.20.4",
    "@types/element-resize-detector": "^1.1.5",
    "@types/google.maps": "^3.54.3",
    "@types/luxon": "^3.3.3",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-svelte": "^2.30.0",
    "luxon": "^3.4.3",
    "p-wait-for": "^5.0.2",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelty-picker": "^5.2.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "vite": "^4.4.2",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
